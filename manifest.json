{
  "manifest_version": 2,
  "name": "ThumbSee",
  "version": "1.4",
  "description": "Displays thumbnails on Odysee download pages",
  "content_scripts": [
    {
      "matches": [
        "*://odysee.com/*"
      ],
      "js": [
        "js/common.js",
        "js/thumbsee.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/common.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}