{
  "manifest_version": 2,
  "name": "ThumbSee",
  "version": "1.2",

  "description": "Displays thumbnails on Odysee",

  "content_scripts": [
    {
      "matches": ["*://odysee.com/*"],
      "js": ["thumbsee.js"]
    }
  ],
  "icons": {
    "16": "./icon/16.png",
    "32": "./icon/32.png",
    "48": "./icon/48.png",
    "128": "./icon/128.png"
  }
}
